{
    "[html][css][sass][scss]": {
        "editor.indentSize": 2,
        "editor.tabSize": 2,
        "editor.formatOnSave": true,
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.zig-cache": true,
        "gofast-*.zip": true,
        "gofast-*.7z": true,
    },
    "cSpell.words": [
        "fatptr",
        "GOFAST",
        "httpz",
        "LLPP",
        "milli",
        "powerline"
    ],
}